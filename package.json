{
  
  "name": "showme_api",
  "version": "1.0.0",
  "scripts": { 
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "migration:create": "sequelize migration:create --name",
    "lint": "eslint src/** __tests__/**",
    "lint:fix": "yarn lint --fix --quiet",
    "createDb": "set NODE_ENV=developement && sequelize db:create",
    "precompile": "rimraf dist",
    "compile": "yarn tsc -p tsconfig-build.json",
    "migration":"sequelize db:migrate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.13.8",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.2",
    "postgres": "^3.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.6.0",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.14.4",
    "@types/validator": "^13.7.13",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
